<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <listener>
  	<listener-class>bankapp.MyListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>Account</servlet-name>
  	<display-name>Account</display-name>
  	<description></description>
  	<servlet-class>bankapp.Account</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Deposit</servlet-name>
  	<display-name>Deposit</display-name>
  	<description></description>
  	<servlet-class>bankapp.Deposit</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Withdraw</servlet-name>
  	<display-name>Withdraw</display-name>
  	<description></description>
  	<servlet-class>bankapp.Withdraw</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>View</servlet-name>
  	<display-name>View</display-name>
  	<description></description>
  	<servlet-class>bankapp.View</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Account</servlet-name>
  	<url-pattern>/Account</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deposit</servlet-name>
  	<url-pattern>/Deposit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Withdraw</servlet-name>
  	<url-pattern>/Withdraw</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>View</servlet-name>
  	<url-pattern>/View</url-pattern>
  </servlet-mapping>
</web-app>
